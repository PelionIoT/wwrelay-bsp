[dram_para]
dram_baseaddr            = 0x40000000
dram_clk                 = 360
dram_type                = 3
dram_rank_num            = 1
dram_chip_density        = 2048
dram_io_width            = 16
dram_bus_width           = 32
dram_cas                 = 6
dram_zq                  = 0x7b
dram_odt_en              = 0
dram_size                = 512
dram_tpr0                = 0x30926692
dram_tpr1                = 0x1090
dram_tpr2                = 0x1a0c8
dram_tpr3                = 0x0
dram_tpr4                = 0x0
dram_tpr5                = 0x0
dram_emr1                = 0x0
dram_emr2                = 0x0
dram_emr3                = 0x0


;*****************************************************************************
; mali configuration
;
; mali_clkdiv            -- clock divisor for mali from pll4
; zzz
; mali_used                = 1	-> 0
